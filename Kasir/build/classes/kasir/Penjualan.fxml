<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="500.0" styleClass="mainFxmlClass" stylesheets="@../style/style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kasir.PenjualanController">
   <children>
      <Button mnemonicParsing="false" style="-fx-background-color: transparant;" />
      <TableView fx:id="tabelPenjualan" layoutX="41.0" layoutY="76.0" prefHeight="133.0" prefWidth="421.0">
        <columns>
          <TableColumn fx:id="kolomIdPenjualan" prefWidth="75.0" text="Id Penjualan" />
          <TableColumn fx:id="kolomKuantitasPenjualan" prefWidth="75.0" text="Kuantitas" />
            <TableColumn fx:id="kolomHargaPenjualan" prefWidth="75.0" text="Harga" />
            <TableColumn fx:id="kolomDiskonPenjualan" prefWidth="75.0" text="Diskon" />
            <TableColumn fx:id="kolomTotalPenjualan" prefWidth="75.0" text="Total" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnEditPenjualan" layoutX="391.0" layoutY="236.0" mnemonicParsing="false" onAction="#bukaEditDataPenjualan" prefHeight="30.0" prefWidth="70.0" styleClass="button2" text="Edit" />
      <Button fx:id="btnDataPenjualankeHalamanUtama" layoutX="41.0" layoutY="239.0" mnemonicParsing="false" onAction="#DataPenjualanKembaliHalamanUtama" style="-fx-background-color: transparant;" text="Kembali" textFill="#fec5bb" />
      <Label layoutX="179.0" layoutY="20.0" text="Data Penjualan" textFill="#fec5bb">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
